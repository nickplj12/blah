config_version=5

[application]

config/name="BLAH! Browser"
run/main_scene="res://Scenes/CEF.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://blah.png"

[autoload]

ControlsSingleton="*res://Scripts/ControlsSingleton.gd"
Utils="*res://Scripts/Utils.gd"
ShaderManager="*res://Scripts/ShaderManager.gd"

[display]

window/size/borderless=true
window/vsync/vsync_mode=3

[editor_plugins]

enabled=PackedStringArray("res://addons/libsm64-godot/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "purple",
"res://Assets/Fonts/": "yellow",
"res://Assets/Misc/": "orange",
"res://Scenes/": "blue",
"res://Scripts/": "yellow",
"res://Shaders/": "teal"
}

